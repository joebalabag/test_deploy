{
	"name": "mdx",
	"version": "1.0.0",
	"description": "API for MDX Web",
	"main": "index.js",
	"scripts": {
		"test": "mocha",
		"gulp": "gulp gulp/gulpfile.js",
		"serve": "nodemon",
		"start": "node index.js",
		"start_staging": "env-cmd -f .env.staging npm run start",
		"start_production": "env-cmd -f .env.production npm run start",
		"pm2:staging": "pm2 start npm --name \"MDX Staging\" --run start_staging",
		"serve:local": "env-cmd -f .env npm run serve",
		"start:local": "env-cmd -f .env npm run start",
		"serve:staging": "env-cmd -f .env.staging npm run serve",
		"start:staging": "env-cmd -f .env.staging npm run start",
		"serve:staging_client": "env-cmd -f .env.staging_client npm run serve",
		"start:staging_client": "env-cmd -f .env.staging_client npm run start",
		"serve:production": "env-cmd -f .env.production npm run serve",
		"start:production": "env-cmd -f .env.production npm run start",
		"migrate:make": "npx knex migrate:make",
		"migrate:latest": "knex migrate:latest",
		"migrate:latest:staging": "env-cmd -f .env.staging knex migrate:latest",
		"migrate:rollback": "knex migrate:rollback",
		"migrate:up": "knex migrate:up",
		"migrate:down": "knex migrate:down",
		"migrate:list": "knex migrate:list",
		"migrate:fresh": "knex migrate:rollback --all && knex migrate:latest && knex seed:run",
		"seed:make": "knex seed:make",
		"seed:run": "knex seed:run"
	},
	"author": "",
	"license": "ISC",
	"dependencies": {
		"array": "^0.4.3",
		"bcrypt": "^5.0.0",
		"body-parser": "^1.19.0",
		"brcypt": "^1.0.1",
		"cors": "^2.8.5",
		"crypto-js": "^4.1.1",
		"date-fns": "^2.29.3",
		"dns-prefetch-control": "^0.3.0",
		"dotenv": "^8.0.0",
		"env-cmd": "^10.1.0",
		"express": "^4.17.1",
		"group-array": "^1.0.0",
		"helmet": "^3.20.0",
		"ip": "^1.1.5",
		"js-sha512": "^0.8.0",
		"jsonwebtoken": "^8.5.1",
		"knex": "^0.95.15",
		"linq": "^4.0.0",
		"mocha": "^6.2.0",
		"moment": "^2.29.4",
		"morgan": "^1.10.0",
		"multer": "^1.4.4",
		"mysql": "^2.18.1",
		"mysql2": "^2.3.0",
		"node-url-shortener": "^1.1.11",
		"nodemon": "^1.19.4",
		"objection": "^3.0.1",
		"platform": "^1.3.6",
		"request-promise": "^4.2.6",
		"socket.io": "^4.4.1",
		"sqlssb": "0.0.2",
		"swagger-ui-express": "^4.3.0",
		"tedious": "^14.3.0",
		"ws": "^7.3.1"
	},
	"devDependencies": {
		"gulp": "^4.0.2",
		"gulp-watch": "^4.2.4"
	}
}
